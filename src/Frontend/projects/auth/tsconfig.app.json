{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "outDir": "../../out-tsc/app",
        "module": "es2015",
        "types": [],
        "paths": {
            "@angular/*": [
                "node_modules/@angular/*"
            ],
            "@auth/*": [
                "projects/auth/src/*"
